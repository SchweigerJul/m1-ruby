<p id="notice"><%= notice %></p>

<h1>Users</h1>

<ul id="users">
<%= render @users %>
</ul>

<%= form_for(@user, remote: true) do |f| %>
  <%= f.label :Username %>
  <%= f.text_field :name %>
  <%= f.label :password %>
  <%= f.password_field :password %>
  <%= f.label :password_confirmation  %>
  <%= f.password_field :password_confirmation %>
  <%= f.submit %>
<% end %>

<br>

<%= link_to 'back to overview', overview_index_path %>

<br>
